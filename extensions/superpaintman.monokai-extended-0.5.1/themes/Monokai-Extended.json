      }
    },
    {
      "name": "Markdown Header 3",
      "scope": [
        "heading.3.markdown"
      ],
      "settings": {
        "foreground": "#D47D19"
      }
    },
    {
      "name": "Markdown Header 4",
      "scope": [
        "heading.4.markdown"
      ],
      "settings": {
        "foreground": "#BF7117"
      }
    },
    {
      "name": "Markdown Header 5",
      "scope": [
        "heading.5.markdown"
      ],
      "settings": {
        "foreground": "#AB6515"
      }
    },
    {
      "name": "Markdown Header 6",
      "scope": [
        "heading.6.markdown"
      ],
      "settings": {
        "foreground": "#965912"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#F92672",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#e42e70",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Quite",
      "scope": [
        "markup.quote.markdown"
      ],
