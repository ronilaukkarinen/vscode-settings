{
  "name": "Monokai Extended",
  "type": "dark",
  "colors": {
    "editor.background": "#272822",
    "editorCursor.foreground": "#F8F8F0",
    "editor.foreground": "#F8F8F2",
    "editor.lineHighlightBackground": "#35352b",
    "editor.selectionBackground": "#49483E",
    "editorWhitespace.foreground": "#6A6A6466",
    "editorGutter.addedBackground": "#A6E22E",
    "editorGutter.deletedBackground": "#F92672",
    "editorGutter.modifiedBackground": "#AE81FF",
    "statusBar.background": "#23241F",
    "activityBar.background": "#23241F",
    "sideBar.background": "#252620"
  },
  "tokenColors": [
    {
      "settings": {
        "background": "#272822",
        "foreground": "#F8F8F2",
        "findHighlight": "#FFE792",
        "findHighlightForeground": "#000000",
        "selectionBorder": "#222218",
        "activeGuide": "#9D550FB0",
        "bracketsForeground": "#F8F8F2A5",
        "bracketsOptions": "underline",
        "bracketContentsForeground": "#F8F8F2A5",
        "bracketContentsOptions": "underline",
        "tagsOptions": "stippled_underline"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "String",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Constant - Built-in",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Constant - User-defined",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Storage type",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#66D9EF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#A6E22E",
        "fontStyle": ""
      }
    },
    {
      "name": "Inherited class",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#FD971F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Library constant",
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Library class",
      "scope": [
        "support.class"
      ],
      "settings": {
        "foreground": "#66D9EF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Library type",
      "scope": [
        "support.type"
      ],
      "settings": {
        "foreground": "#66D9EF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Library variable",
      "scope": [
        "support.variable"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Library property",
      "scope": [
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "background": "#F92672",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "background": "#AE81FF",
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Object Key",
      "scope": [
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "Object Key-Value Separator",
      "scope": [
        "punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Punctuation Accessor",
      "scope": [
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Template Expression",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Template Expression Brakets",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#E69F66"
      }
    },
    {
      "scope": [
        "constant.numeric.line-number.find-in-files - match"
      ],
      "settings": {
        "foreground": "#AE81FFA0"
      }
    },
    {
      "scope": [
        "entity.name.filename.find-in-files"
      ],
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "name": "JavaScript - this",
      "scope": [
        "variable.language.this.ts",
        "variable.language.this.js",
        "variable.language.this.tsx",
        "variable.language.this.jsx"
      ],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "scope": [
        "support.class.component.js",
        "support.class.component.jsx",
        "support.class.component.ts",
        "support.class.component.tsx"
      ],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "JSON String",
      "scope": [
        "meta.structure.dictionary.json",
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#CFCFC2"
      }
    },
    {
      "name": "JSON Property",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#F92672",
        "fontStyle": ""
      }
    },
    {
      "name": "Clojure - defs",
      "scope": [
        "keyword.control.clojure"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Clojure - Keyword",
      "scope": [
        "constant.keyword.clojure"
      ],
      "settings": {
        "foreground": "#AE81FF"
      }
    },
    {
      "name": "Go - func",
      "scope": [
        "keyword.function.go"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Go - var",
      "scope": [
        "keyword.var.go"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Go - type",
      "scope": [
        "keyword.type.go"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Go Function",
      "scope": [
        "support.function.go"
      ],
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Go Period",
      "scope": [
        "punctuation.other.period.go"
      ],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Go Placeholder",
      "scope": [
        "constant.other.placeholder.go"
      ],
      "settings": {
        "foreground": "#E69F66"
      }
    },
    {
      "name": "Markdown Headers",
      "scope": [
        "markup.heading.markdown",
        "markup.heading.setext.1.markdown",
        "markup.heading.setext.2.markdown"
      ],
      "settings": {
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Markdown Header 1",
