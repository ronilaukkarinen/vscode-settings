{
  "Media query ": {
    "prefix": "media",
    "body": [
      "@media (max-width: ${1:}) {",
      "  ${2:}",
      "}",
    ],
  },
  "Media query for mobile": {
    "prefix": "min",
    "body": [
      "@media (min-width: \\$container-mobile) {",
      "  ${1:}",
      "}",
    ],
  },
  "Media query for iPad": {
    "prefix": "ipad",
    "body": [
      "@media (max-width: \\$container-ipad) {",
      "  ${1:}",
      "}",
    ],
  },
  "Grid Template Columns": {
    "prefix": "gridtemplate",
    "body": [
      "grid-template-columns: 605px minmax(0, 1fr);",
    ],
  },
  "CSS Grid": {
    "prefix": "grid",
    "body": [
      "@media (min-width: \\$container-ipad-landscape) {",
      "  display: grid;",
      "  grid-template-columns: ${1:1fr 1fr};",
      "}",
    ],
  },
  "Responsive font": {
    "prefix": "font",
    "body": [
      "@include responsive-font(22px, ${1:64}px);",
    ],
  },
}
