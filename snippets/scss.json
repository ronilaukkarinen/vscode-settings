    "Media query": {
        "prefix": "media",
        "body": [
            "@media(max-width: ${1:770}px) {",
			"	"
			"}",
        ],
    },
    "CSS Grid": {
        "prefix": "grid",
        "body": [
            "@include grid(${1:3}, \\$gutter_x: 50px, \\$gutter_y: 50px);",
        ],
    }
}