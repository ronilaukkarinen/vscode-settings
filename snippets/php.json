"Get ACF field": {
  "prefix": "field",
  "body": [
    "$$1 = get_field( '$1'$0 );",
  ],
  "description": "Get ACF field"
},
"Get ACF subfield": {
  "prefix": "sub",
  "body": [
    "$$1 = get_sub_field( '$1'$0 );",
  ],
  "description": "Get ACF subfield"
},
"Echo URL": {
  "prefix": "eurl",
  "body": [
    "echo esc_url( $1 );",
  ],
  "description": "Echo URL"
},
"Echo HTML": {
  "prefix": "ehtml",
  "body": [
    "echo esc_html( $1 );",
  ],
  "description": "Echo HTML"
},
"Echo attr": {
  "prefix": "eattr",
  "body": [
    "echo esc_attr( $1 );",
  ],
  "description": "Echo attr"
},
"Echo content": {
  "prefix": "econtent",
  "body": [
    "echo wp_kses_post( $1 );",
  ],
  "description": "Echo content"
},
}