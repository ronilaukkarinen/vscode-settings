{
	"name": "file-size",
	"displayName": "file-size",
	"description": "Show the current text file size in the status bar.",
	"version": "0.1.3",
	"publisher": "zh9528",
	"engines": {
		"vscode": "^1.18.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/zh28/file-size"
	},
	"keywords": [
		"file-size",
		"filesize",
		"file",
		"size",
		"status bar"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"license": "MIT",
	"icon": "icon.png",
	"main": "./extension",
	"contributes": {
		"configuration": [
			{
				"title": "file-size configuration",
				"properties": {
					"file-size.position": {
						"type": "string",
						"default": "right",
						"description": "set left or right  设置显示位置. E.g \"file-size.position\":\"left\""
					},
					"file-size.priority": {
						"type": "integer",
						"default": 0,
						"description": "Higher values mean the item should be shown more to the left. 设置优先级，值越大越往左"
					}
				}
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^7.0.43",
		"eslint": "^4.6.1",
		"typescript": "^2.6.1",
		"vscode": "^1.1.10"
	},
	"__metadata": {
		"id": "9734a8c6-1c74-4259-92ba-e4c3edf4a483",
		"publisherId": "71280a6f-7240-4520-8420-569f9e905dd1",
		"publisherDisplayName": "zhcode"
	}
}