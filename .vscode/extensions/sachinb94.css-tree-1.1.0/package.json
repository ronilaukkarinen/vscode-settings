{
	"name": "css-tree",
	"displayName": "CSS Tree",
	"description": "Generate CSS tree from selected HTML/JSX",
	"version": "1.1.0",
	"publisher": "sachinb94",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"CSS tree",
		"SASS tree",
		"LESS tree",
		"HTML",
		"JSX"
	],
	"activationEvents": [
		"onCommand:extension.generateCssTree"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.generateCssTree",
				"title": "Generate CSS tree"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Generate CSS Tree configuration",
			"properties": {
				"generateCssTree.cssFlavor": {
					"type": "string",
					"default": "sass",
					"description": "CSS flavor"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"document-tree": "^1.0.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sachinB94/vscode-css-tree.git"
	},
	"license": "MIT",
	"__metadata": {
		"id": "91079b50-d3ef-43ae-9f1b-ed14b5ccb167",
		"publisherId": "2d0b299f-bd5f-4eed-bcf5-4c4e015c0335",
		"publisherDisplayName": "Sachin Bansal"
	}
}