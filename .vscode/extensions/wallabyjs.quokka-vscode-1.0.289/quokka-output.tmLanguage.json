{
  "scopeName": "wallaby.output",
  "name": "wallaby-output",
  "patterns": [
    {
      "comment": "deletion",
      "match": "‍{1}[^‍]+‍{1}",
      "name": "markup.deleted markup.underline"
    },
    {
      "comment": "insertion",
      "match": "⁠{1}[^⁠]+⁠{1}",
      "name": "markup.inserted markup.underline"
    },
    {
      "comment": "message",
      "match": "\u200B{5}[^\u200B]+\u200B{5}",
      "name": "constant.numeric.js"
    },
    {
      "comment": "message",
      "match": ".*(\u2008|\u0020\u200B{5})",
      "name": "constant.numeric.js"
    },
    {
      "comment": "suite",
      "match": "\u200B{4}[^\u200B]+\u200B{4}",
      "name": "string.value"
    },
    {
      "comment": "duration",
      "match": "\u200B{3}[^\u200B]+\u200B{3}",
      "name": "entity.name.function"
    },
    {
      "comment": "errors",
      "match": "\u200B{2}[^\u200B]+\u200B{2}",
      "name": "invalid"
    },
    {
      "comment": "errors",
      "match": ".*\u2007",
      "name": "invalid"
    },
    {
      "comment": "link",
      "match": "\u200B{1}[^\u200B]+\u200B{1}",
      "name": "storage.type"
    }
  ]
}
