{
	"scopeName": "propfix.injection",
	"injectionSelector": "L:meta.property-list.scss",
	"patterns": [
		
		{
			"include": "#selector" 
		}
		
	],
	"repository": {
		"selector": {
			"match": "(?xi)((content|cursor|filter|font|mask)(\\s*)(:+))(?!(?: after|before|first-letter|first-line|backdrop|content|grammar-error|marker|placeholder|selection|shadow|spelling-error|active|any-link|checked|default|disabled|empty|enabled|first|(?:nth|nth-last|first|last|only)-(?: child|of-type)|focus|focus-within|fullscreen|host|hover|in-range|indeterminate|invalid|left|link|optional|out-of-range|read-only|read-write|required|right|root|scope|target|unresolved|valid|visited)|\\s*{)",
			"name": "meta.property-name.scss support.type.property-name.css"
		}
	}
}
