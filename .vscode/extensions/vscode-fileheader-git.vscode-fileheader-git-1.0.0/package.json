{
	"name": "vscode-fileheader-git",
	"displayName": "vscode-fileheader-git",
	"description": "insert header comment with git config, and automatically update the time.",
	"version": "1.0.0",
	"publisher": "vscode-fileheader-git",
	"author": {
		"name": "mikey.zhaopeng",
		"url": "http://zhaopeng.me"
	},
	"galleryBanner": {
		"color": "#3B4859",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/zhaopengme/vscode-fileheader.git"
	},
	"bugs": {
		"url": "https://github.com/zhaopengme/vscode-fileheader/issues",
		"email": "imzhpe@qq.com"
	},
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.fileheadergit",
		"onLanguage:typescript",
		"onLanguage:typescriptreact"
	],
	"main": "./dist/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "File header Configuration",
			"properties": {
				"fileheadergit.tpl": {
					"type": "string",
					"default": "/*\r\n * @Author: {author}\r\n * @Email: {email}\r\n * @Date: {createTime}\r\n * @Last Modified by:   {lastModifiedBy}\r\n * @Last Modified time: {updateTime}\r\n * @Description: Description\r\n */\r\n",
					"description": "By default, common template. Do not modify it!!!!!"
				}
			}
		},
		"commands": [
			{
				"command": "extension.fileheadergit",
				"title": "fileheadergit"
			}
		],
		"keybindings": [
			{
				"command": "extension.fileheadergit",
				"key": "ctrl+alt+i",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"dev": "tsc -w",
		"build": "webpack --config=webpack.config.js",
		"vscode:prepublish": "npm run build"
	},
	"devDependencies": {
		"@types/node": "^12.6.1",
		"@types/vscode": "^1.36.0",
		"ts-loader": "^6.0.4",
		"typescript": "^3.5.3",
		"vscode": "^0.11.0",
		"webpack": "^4.35.3",
		"webpack-cli": "^3.3.5",
		"webpack-node-externals": "^1.7.2"
	},
	"dependencies": {
		"fs-extra": "^8.1.0",
		"promise-exec": "^0.1.0"
	},
	"__metadata": {
		"id": "12b447c6-a1ca-444a-8250-67ccce77d577",
		"publisherId": "a97f02d7-2e38-41d6-a694-a7b8ed9468ed",
		"publisherDisplayName": "kage336"
	}
}