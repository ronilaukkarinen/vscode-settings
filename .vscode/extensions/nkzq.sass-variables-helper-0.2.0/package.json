{
	"name": "sass-variables-helper",
	"displayName": "Sass Variables Helper",
	"description": "A Tree View of your Sass colors variables",
	"version": "0.2.0",
	"publisher": "Nkzq",
	"icon": "media/icon.png",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:sassVariables"
	],
	"main": "./out/src/extension",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "package-explorer",
					"title": "SASS Variables",
					"icon": "media/color.svg"
				}
			]
		},
		"views": {
			"package-explorer": [
				{
					"id": "sassVariables",
					"name": "Colors"
				}
			]
		},
		"commands": [
			{
				"command": "sassVariables.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "sassVariables.refreshEntry",
					"when": "view == sassVariables",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "SASS Variables Helper",
			"properties": {
				"sassVariablesHelper.route": {
					"type": "string",
					"default": false,
					"description": "Set the route of your sass variables files"
				}
			}
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Nkzq/sass-variables-helper"
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.1.4",
		"vscode": "^1.1.17",
		"@types/node": "*",
		"@types/mkdirp": "^0.5.2",
		"@types/rimraf": "^2.0.2"
	},
	"dependencies": {
		"copy-paste": "^1.3.0",
		"ftp": "^0.3.10",
		"jsonc-parser": "^0.4.2",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2"
	},
	"__metadata": {
		"id": "5258a2f3-6ab4-46cc-bdca-42a485ee209b",
		"publisherId": "17bbbcbc-4246-404d-ae01-bbeca847f2a9",
		"publisherDisplayName": "Nkzq"
	}
}