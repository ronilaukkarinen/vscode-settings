{
	"name": "copywithline",
	"displayName": "Copy with line numbers and file name.",
	"description": "Just let you can copy selected code with line numbers and file name.",
	"version": "0.0.2",
	"publisher": "diigu",
	"repository": "https://github.com/diigu/vscode-copywithlinenumbers",
	"bugs": "https://github.com/diigu/vscode-copywithlinenumbers/issues",
	"engines": {
		"vscode": "^1.16.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.copywithline"
	],
	"main": "./out/src/extension",
	"contributes": {
		"keybindings": [
			{
				"command": "extension.copywithline",
				"key": "Ctrl+Shift+C"
			}
		],
		"commands": [
			{
				"command": "extension.copywithline",
				"title": "Copy with line number"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"dependencies": {
		"copy-paste": "^1.3.0"
	},
	"devDependencies": {
		"typescript": "^2.5.2",
		"vscode": "^1.1.5",
		"mocha": "^3.5.0",
		"@types/node": "^7.0.43",
		"@types/mocha": "^2.2.42"
	},
	"__metadata": {
		"id": "e80dbab7-a472-4b53-8307-5596da0de585",
		"publisherId": "bd5a3412-2723-451a-b649-2523850ab4cc",
		"publisherDisplayName": "diigu"
	}
}