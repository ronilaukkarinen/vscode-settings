{
	"name": "vscode-svgo",
	"displayName": "SVGO",
	"description": "Use SVGO to optimise SVG files from within VS Code",
	"version": "0.1.0",
	"publisher": "ajhyndman",
	"author": "Andrew Hyndman <ajhyndman@hotmail.com>",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.16.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.optimizeActiveFile",
		"onCommand:extension.optimizeSelection"
	],
	"main": "./out/src/extension",
	"license": "MIT",
	"private": "true",
	"repository": {
		"type": "git",
		"url": "https://github.com/ajhyndman/vscode-svgo.git"
	},
	"bugs": {
		"url": "https://github.com/ajhyndman/vscode-svgo/issues"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.optimizeActiveFile",
				"title": "SVGO: Optimize current file"
			},
			{
				"command": "extension.optimizeSelection",
				"title": "SVGO: Optimize current selection"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.5.2",
		"vscode": "^1.1.5",
		"mocha": "^3.5.0",
		"@types/node": "^7.0.43",
		"@types/mocha": "^2.2.42"
	},
	"__metadata": {
		"id": "bdb4c8d2-b2f8-45b1-a5b6-f15c223967c5",
		"publisherId": "1a1c9dc0-f1fd-4305-82e6-16261926652a",
		"publisherDisplayName": "Andrew Hyndman"
	}
}